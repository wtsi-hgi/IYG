<TMPL_INCLUDE NAME="../includes/result_header.tmpl">
  <div class="variants">
    <div class="trait">
      <div class="trait-header">
        <b><h2><TMPL_VAR NAME=TRAIT_NAME></h2></b>
      </div>
      <div class="row-fluid">
        <div class="accordion-inner">
          <p><TMPL_VAR NAME=TRAIT_DESCRIPTION></p>
        </div>
      </div>
      <div class="row-fluid">
	<div class="span2"></div>
	<div class="span8">
	  <div class="row-fluid">
	    <div class="span12">
	      <img src="<TMPL_VAR NAME=TREE_URI>" alt="Your Tree"/>
	    </div>
	  </div>
	  <TMPL_IF NAME=TREE_DESC>
	  <div class="row-fluid">
	    <div class="span12 center">
	      <TMPL_VAR NAME=TREE_DESC>
	    </div>
	  </div>
	  </TMPL_IF>
	</div>
	<div class="span2"></div>
      </div>
    </div>
    <TMPL_LOOP NAME=SNPS>
      <TMPL_INCLUDE NAME="snp_box.tmpl">
    </TMPL_LOOP>
  </div>
<TMPL_INCLUDE NAME="../includes/footer.tmpl">
    
